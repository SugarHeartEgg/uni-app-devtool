page {
  background-color: #fff;
  font-size: 28rpx;
}
.content .u-navbar {
  background-color: var(--nav-bg) !important;
}
.content .u-navbar .u-status-bar {
  background-color: #fff !important;
}
.content .main-warp  .list-warp {
  padding: 0 10rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.content .main-warp  .list-warp .logo {
  height: 200rpx;
  width: 200rpx;
  margin-top: 200rpx;
  margin-left: auto;
  margin-right: auto;
  margin-bottom: 50rpx;
}
.content .main-warp  .list-warp .text-area {
  display: flex;
  justify-content: center;
}
.content .main-warp  .list-warp .title {
  font-size: 36rpx;
  color: #8f8f94;
}
.content .main-warp  .list-warp .card-touch {
  display: flex;
  flex-direction: column;
}
.content .main-warp  .list-warp .card-touch .card-main {
  width: 100%;
  height: 200rpx;
  transition-property: height;
  transition-duration: 800ms;
  transition-timing-function: ease;
  background: var(--nav-bg) !important;
}
.content .main-warp  .list-warp .card-touch .card-main.touch-card-main {
  height: 700rpx;
}
.content .main-warp  .list-warp .card-touch .card-line {
  padding: 10rpx 0;
}
.content .main-warp  .list-warp .h-flex-x {
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: flex-start;
  align-items: center;
  align-content: center;
}
.content .main-warp  .list-warp .card {
  background-color: var(--nav-bg);
  color: var(--nav-color);
  padding: 30rpx;
}
.content .main-warp  .list-warp .list-item {
  height: 90rpx;
  border-top: var(--nav-color) solid 1px;
}
.content .main-warp  .list-warp .nav-bar {
  box-sizing: content-box;
  height: 44px;
  background-color: var(--nav-bg);
  padding-top: 25px;
  color: var(--nav-color);
  line-height: 44px;
  text-align: center;
  font-size: 16px;
}
.content .main-warp  .list-warp .btn {
  background-color: var(--nav-bg) !important;
  color: var(--nav-color) !important;
}
.content .main-warp  .list-warp .text-color {
  color: var(--text-color) !important;
}
.content .main-warp  .list-warp .charts-box {
  width: 100%;
  height: 300px;
}

